{
  "manifest_version": 3,
  "name": "QR Code Generator",
  "short_name": "QR Generator",
  "version": "1.0",
  "description": "Aplikasi untuk menghasilkan QR Code.",
  "icons": {
    "16": "icon/icon-16x16.png",
    "48": "icon/icon-48x48.png",
    "128": "icon/icon-128x128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon/icon-16x16.png",
      "48": "icon/icon-48x48.png",
      "128": "icon/icon-128x128.png"
    }
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "activeTab"
  ]
}
